	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + customerId;
		return result;
	}
	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		RetailCustomerMasterBean other = (RetailCustomerMasterBean) obj;
		if (customerId != other.customerId)
			return false;
		return true;
	}
